{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": [
      false
    ],
    "ordered-imports": [false],
    "quotemark": [true, "single"],
    "no-console": false,
    "max-classes-per-file": false,
    "variable-name": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/healthcheck.js",
      "coverage/**",
      "src/utils/BizRule/**",
      "src/migration/*.ts",
      "**/*/*.spec.ts"
    ]
}
}