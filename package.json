{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "clean": "rimraf out/*",
    "build": "npm run clean && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "NODE_ENV=production node out/src/index.js",
    "test": "jest --config=jest.config.json --maxWorkers=3 --forceExit --coverage",
    "dev:nowatch": "npm run lint && ts-node src/index.ts",
    "dev:run": "ts-node src/index.ts",
    "dev": "nodemon --watch src -e ts --exec npm run dev:nowatch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/config/migration.ts",
    "migrate:up": "npm run typeorm migration:run -- -c ea",
    "migrate:down": "npm run typeorm migration:revert -- -c ea",
    "migrate:generate": "npm run typeorm migration:generate -- -c ea -n test",
    "migrate:create": "npm run typeorm migration:create -- -c ea"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.10",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "nodemon": "^1.3.3",
    "ts-jest": "^28.0.5",
    "ts-node": "10.7.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "^6.1.3",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "jest": "^28.1.2",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.5.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.2.4",
    "typeorm": "^0.2.29",
    "typeorm-snake-naming-strategy": "^1.0.2"
  }
}
